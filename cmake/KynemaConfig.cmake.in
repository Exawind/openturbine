@PACKAGE_INIT@

include(CMakeFindDependencyMacro)
include("${CMAKE_CURRENT_LIST_DIR}/KynemaTargets.cmake")

check_required_components(kynema_library)

find_dependency(KokkosKernels REQUIRED)
find_dependency(yaml-cpp REQUIRED)
find_dependency(NetCDF REQUIRED)
find_dependency(LAPACK REQUIRED)

if(@Kynema_ENABLE_SUPERLU@)
  find_dependency(superlu REQUIRED)
endif()

if(@Kynema_ENABLE_SUPERLU_MT@)
  find_dependency(superlu_mt REQUIRED)
endif()

if(@Kynema_ENABLE_KLU@)
  find_dependency(KLU REQUIRED)
endif()

if(@Kynema_ENABLE_UMFPACK@)
  find_dependency(UMFPACK REQUIRED)
endif()

if(@Kynema_ENABLE_MKL@)
  find_dependency(MKL REQUIRED)
endif()

if(@Kynema_ENABLE_CUDSS@)
  find_dependency(cudss REQUIRED)
endif()

